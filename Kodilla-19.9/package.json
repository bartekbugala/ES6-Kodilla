{
  "name": "github-search",
  "version": "1.0.0",
  "description": "Github user search app using react",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all -p watch:* build",
    "build": "babel script.js --watch --out-file build.js",
    "watch:browsersync": "browser-sync start --server --files js css *.html"
