    "search",
    "react",
    "ES6"
  ],
  "author": "BB",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bartekbugala/ES6-Kodilla/issues"
  },
  "homepage": "https://github.com/bartekbugala/ES6-Kodilla#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "browser-sync": "^2.26.5",
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {}
}
